{
  "env": {
    "node": true,
    "commonjs": true,
    "jest": true,
    "es2019": true
  },
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended", "google", "prettier"],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "root": true,
  "rules": {
    "@typescript-eslint/no-non-null-assertion": "off",
    "camelcase": "off"
  }
}
